{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "silex/silex": "*",
        "symfony/console": "^4.3",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "^4.3",
        "symfony/translation": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {

    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
            "App\\": "src/",
            "Inviqa\\": "src/Inviqa/",
            "RgpJones\\": "src/RgpJones/",
            "spec\\RgpJones\\": "spec/RgpJones/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "spec": [
            "vendor/bin/phpspec run"
        ],
        "sniff": [
            "vendor/bin/phpcs --standard=PSR2 src/"
        ],
        "fix": [
            "vendor/bin/phpcbf --standard=PSR2 src/"
        ],
        "test": [
            "@sniff",
            "@spec"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony-web-dir": "web",
        "public-dir": "web",
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
